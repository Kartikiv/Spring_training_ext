queryForInsertStudentDetails=insert into student_details_trans (first_nm,middle_nm,last_nm,mobile_nm,address, city_nm,town_nm,district_nm,village_nm,birth_date,org_id) values(:firstNm,:middleNm,:lastNm,:mobileNm,:address,:cityNm,:townNm,:districtNm,:villageNm,:birthDate,:orgId);
queryForUpdateStudentDetails=update student_details_trans set first_nm= :firstNm,middle_nm= :middleNm,last_nm= :lastNm,mobile_nm= :mobileNm,address= :address, city_nm= :cityNm,town_nm= :townNm,district_nm= :districtNm,village_nm= :villageNm,birth_date= :birthDate,modified_date=now(),version_inf=version_inf+1 where student_id= :studentId and org_id= :orgId and isactive=true; 
queryForInsertJdbc=insert into student_details_trans(first_nm,middle_nm,last_nm,mobile_nm,address, city_nm,town_nm,district_nm,village_nm,birth_date,org_id) values(?,?,?,?,?,?,?,?,?,?,?);
queryForInsertHistoryTable=insert into student_details_trans_history (student_id,first_nm,middle_nm,last_nm,mobile_nm,address, city_nm,town_nm,district_nm,village_nm,birth_date,org_id,created_date,modified_date,version_inf,isactive) select student_id,first_nm,middle_nm,last_nm,mobile_nm,address, city_nm,town_nm,district_nm,village_nm,birth_date,org_id,created_date,modified_date,version_inf,isactive from student_details_trans where org_id=:orgId and student_id=:studentId ;
queryForUpdateJdbc=update student_details_trans set first_nm=?,middle_nm=?,last_nm=?,mobile_nm=?,address=?, city_nm=?,town_nm=?,district_nm=?,village_nm=?,birth_date=?,modified_date=now(),version_inf=version_inf+1 where student_id=? and org_id=? and isactive=true; 
queryForInsertHistoryTableJdbc=insert into student_details_trans_history (student_id,first_nm,middle_nm,last_nm,mobile_nm,address, city_nm,town_nm,district_nm,village_nm,birth_date,org_id,created_date,modified_date,version_inf,isactive) select student_id,first_nm,middle_nm,last_nm,mobile_nm,address, city_nm,town_nm,district_nm,village_nm,birth_date,org_id,created_date,modified_date,version_inf,isactive from student_details_trans where org_id=? and student_id=? ;
queryForStudentDetailsUsingId=select student_id,concat(first_nm,' ',middle_nm,' ',last_nm) as nm,address,city_nm,town_nm,district_nm,village_nm,mobile_nm,birth_date,org_id,created_date,modified_date,version_inf from student_details_trans where org_id=:orgId and isactive=true and student_id=:studentId
queryForStudentDetailsUsingQuery=select student_id,concat(first_nm,' ',middle_nm,' ',last_nm) as nm,mobile_nm,address,city_nm,town_nm,district_nm,village_nm,birth_date,org_id,created_date,modified_date,version_inf from student_details_trans where org_id=:orgId and isactive=true 
queryForStudentDetailsWithMarks=select concat(first_nm,' ',last_nm ) as name ,address,city_nm ,total_marks,stage,s.student_id,marks_id from student_details_trans s left join student_details_marks m on s.student_id=m.student_id where org_id=1 and isactive=true order by student_id
queryForInsertXrefTable=insert into student_department_xref_trans(student_id,department_id,org_id) values(:studentId,:departmentId,:orgId)
queryforStudentDetailsWithMarksAndDepartment=select  concat(first_nm,' ',last_nm ) as name ,e.address,e.city_nm ,m.total_marks,stage,e.student_id,m.marks_id,d.department_id,d.department_namefrom student_details_trans e join student_details_marks m on m.student_id = e.student_id join student_department_xref_trans s on s.student_id = e.student_id join student_department_trans d on d.department_id = s.department_id where e.org_id=1 and e.isactive=true order by e.student_id